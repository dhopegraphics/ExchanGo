{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*",
        "../*",
         "../../*"
      ]
    } ,
    "types": ["@types/d3-path"]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.js",
    "**/*.jsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
, "app/_layout.jsx", "app/index.jsx" , "app/*" , 
"Context/ThemeContext.tsx", 
"app/(Root)/(tabs)/home", 
"app/(Root)/Settings/securityRoom" , 
"app/Chat/*",
"lib/*",
"Context/*",
"hooks/*",
"constants/*",
"components/*"
]
}
